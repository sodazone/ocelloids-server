{
  "singleQuote": true,
  "printWidth": 120,
  "overrides": [
    {
      "files": "*.ts,*.js",
      "options": {
        "htmlWhitespaceSensitivity": "strict"
      }
    },
    {
      "files": "*.css",
      "options": {
        "parser": "css",
        "singleQuote": true,
        "printWidth": 200
      }
    },
    {
      "files": "*.md",
      "options": {
        "printWidth": 200
      }
    }
  ]
}
