{
  "name": "@sodazone/ocelloids-service-node",
  "version": "1.0.0",
  "type": "module",
  "description": "Ocelloids Service Node",
  "author": "SO/DA <projects@soda.zone>",
  "contributors": [
    "Marc Forn√≥s <marc@soda.zone>",
    "Xueying Wang <xueying@soda.zone>"
  ],
  "main": "./dist/lib.js",
  "types": "./dist/lib.d.ts",
  "bin": {
    "oc-ingress": "./dist/services/ingress/server/main.js",
    "oc-node": "./dist/main.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/main.js",
    "dev": "nodemon --exec tsx ./src/main.ts",
    "dev:inspect": "nodemon --exec tsx --inspect ./src/main.ts",
    "dev:ingress": "nodemon --exec tsx ./src/services/ingress/server/main.ts",
    "dev:exec": "nodemon --exec tsx ./src/main.ts --distributed",
    "lint": "biome check --apply src/**/*.ts",
    "test": "NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest"
  },
  "license": "Apache-2.0",
  "private": true,
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      ".*/dist",
      "testing",
      "src/services/telemetry/metrics",
      "src/services/notification/log.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(web-worker)/)"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": [
        "@swc/jest"
      ]
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@biomejs/biome": "1.7.3",
    "@swc/core": "^1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/ws": "^8.5.10",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "memory-level": "^1.0.0",
    "nock": "^13.5.4",
    "nodemon": "^3.1.1",
    "patch-package": "^8.0.0",
    "pino-pretty": "^11.1.0",
    "ts-jest": "^29.1.3",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/websocket": "^10.0.1",
    "@sodazone/ocelloids-sdk": "^1.2.5",
    "async-mutex": "^0.5.0",
    "cbor-x": "^1.5.9",
    "close-with-grace": "^1.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.27.0",
    "fastify-healthcheck": "^4.4.0",
    "fastify-plugin": "^4.5.1",
    "got": "^14.3.0",
    "handlebars": "^4.7.8",
    "level": "^8.0.1",
    "lru-cache": "^10.2.2",
    "prom-client": "^15.1.2",
    "redis": "^4.6.14",
    "rfc6902": "^5.1.1",
    "toml": "^3.0.0",
    "ulidx": "^2.3.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.0"
  },
  "packageManager": "yarn@4.1.0"
}
